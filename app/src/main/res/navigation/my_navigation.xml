<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/my_navigation"    app:startDestination="@id/splashScreenFragment">    <fragment        android:id="@+id/registrationFragment"        android:name="com.abdurashidov.chatting.ui.RegistrationFragment"        android:label="fragment_registration"        tools:layout="@layout/fragment_registration" >        <action            android:id="@+id/action_registrationFragment_to_usersFragment"            app:destination="@id/usersFragment" />    </fragment>    <fragment        android:id="@+id/usersFragment"        android:name="com.abdurashidov.chatting.ui.UsersFragment"        android:label="fragment_users"        tools:layout="@layout/fragment_users" >        <action            android:id="@+id/action_usersFragment_to_messageFragment"            app:destination="@id/messageFragment" />    </fragment>    <fragment        android:id="@+id/messageFragment"        android:name="com.abdurashidov.chatting.ui.MessageFragment"        android:label="fragment_message"        tools:layout="@layout/fragment_message" />    <fragment        android:id="@+id/splashScreenFragment"        android:name="com.abdurashidov.chatting.ui.SplashScreenFragment"        android:label="fragment_splash_screen"        tools:layout="@layout/fragment_splash_screen" >        <action            android:id="@+id/action_splashScreenFragment_to_registrationFragment"            app:destination="@id/registrationFragment" />        <action            android:id="@+id/action_splashScreenFragment_to_usersFragment"            app:destination="@id/usersFragment" />        <action            android:id="@+id/action_splashScreenFragment_to_writePhoneFragment"            app:destination="@id/writePhoneFragment" />    </fragment>    <fragment        android:id="@+id/phoneAuthFragment"        android:name="com.abdurashidov.chatting.ui.PhoneAuthFragment"        android:label="fragment_phone_auth"        tools:layout="@layout/fragment_phone_auth" >        <action            android:id="@+id/action_phoneAuthFragment_to_addInfoRegisterFragment"            app:destination="@id/addInfoRegisterFragment" />    </fragment>    <fragment        android:id="@+id/writePhoneFragment"        android:name="com.abdurashidov.chatting.ui.WritePhoneFragment"        android:label="fragment_write_phone"        tools:layout="@layout/fragment_write_phone" >        <action            android:id="@+id/action_writePhoneFragment_to_phoneAuthFragment"            app:destination="@id/phoneAuthFragment" />    </fragment>    <fragment        android:id="@+id/addInfoRegisterFragment"        android:name="com.abdurashidov.chatting.ui.AddInfoRegisterFragment"        android:label="fragment_add_info_register"        tools:layout="@layout/fragment_add_info_register" >        <action            android:id="@+id/action_addInfoRegisterFragment_to_usersFragment"            app:destination="@id/usersFragment" />    </fragment></navigation>